@model IEnumerable<UCPPraktikam1.Models.Pembayaran>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Total Pembayaran")
            </th>
            <th>
                @Html.DisplayName("Id tranksaksi Navigation")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Totalembayaran)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdtranksaksiNavigation.Idtransaksi)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idpembayaran"><i class="fa fa-marker fa-lg"></i></a> |
                <a asp-action="Details" asp-route-id="@item.Idpembayaran"><i class="fa fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.Idpembayaran"><i class="fa fa-trash-alt fa-lg"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
